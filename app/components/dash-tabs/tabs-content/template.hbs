{{#blackout-tabs
  isSubTabs=isSubTabs
  parentTab=parentTab
  resetOn=(readonly resetOn)
  receiveTabs=receiveTabs
  selectedTab=(readonly selectedTab)
  onTabChange=tabChanged
  selectTab=selectTab
  tabRegistration=tabRegistration
  hideTabs=true
  tabGroup=tabGroup
  tabRoute=tabRoute
  isOnScreen=(readonly isOnScreen)
  loaderColor=(if boxless 'onDark' 'light')
  defaultTab=defaultTab
  as |tabs| }}
  
  {{yield tabs}}
  
{{/blackout-tabs}}
